{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
   "hostRules": [
        {
            "matchHost": "my-registry.io",
            "enabled": true
        },
        {
            "matchHost": "my-registry1.io",
            "enabled": true
        },
        {
            "matchHost": "quay.io",
            "enabled": false
        },
        {
            "matchHost": "docker.io",
            "enabled": false
        }
    ],
    "packageRules": [
        {
            "matchDatasources": [
                "docker"
            ],
            "registryAliases":
            {
                "quay.io": "https://my-registry.io",
                "docker.io": "http://my-registry1.io"
            }
        }
    ]
}
